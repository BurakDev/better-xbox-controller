{
  "name": "better-xbox-controller",
  "version": "0.1.3",
  "description": "Library for easy access to XBox 360 Controller",
  "keywords": [
    "xbox",
    "360",
    "controller",
    "usb"
  ],
  "main": "src/controller",
  "scripts": {
    "build": "coffee -c src/*.coffee && coffee -c example/*.coffee",
    "clean": "rm src/*.js && rm example/*.js",
    "pretest": "npm run-script build",
    "test": "node example/example",
    "prepublish": "npm run-script build"
  },
  "author": "Stefan Steininger",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dreistein/better-xbox-controller"
  },
  "license": "MIT",
  "dependencies": {
    "usb": "^1.1.1"
  }
}
